#pragma hrstop
#pragma argsused

#include <stdio.h>
#include <conio.h>
#include <math.h>

#ifdef _WIN32
#include <tchar.h>
#endif
int puissance(int a, int b)
{
	int calcul;
	calcul=pow(a,b);

	return calcul;
}

int main(int argc, char* argv[])
{
	int x,y,a,b,calcul;
	printf("Quelle est la valeur de x ? \n");
	scanf("%d",&x);
	printf("Quelle est la valeur de y ? \n");
	scanf("%d",&y);

	calcul=puissance(x,y);

	printf("La valeur est égale à %d",calcul);

	getch();
	return 0;
}
