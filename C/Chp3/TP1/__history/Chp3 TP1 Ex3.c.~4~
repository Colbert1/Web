#include <stdio.h>
#include <stdlib.h>
#include <tchar.h>

         float min (float*tab)
         {
			   float valuemin = 100000;
              int i;
              
              for(i = 0; i < 8; i++)
              {
					if(tab[i]<valuemin)
                    {
					  valuemin = tab[i];
                    }
              }      
                   
				return valuemin;
              
         }
         
         float max (float*tab)
         { 
			   float valuemax= -100000;
               int i;
               
               
               for(i = 0; i < 8; i++)
               {
					 if (tab[i]>valuemax)
                     {
					  valuemax = tab[i];
                     }
               }    
				 return valuemax;
               
         }
         
          void main()
          
         {
