#include <stdio.h>
#include <tchar.h>
#include <stdlib.h>

int trier(int *tab)
{
	int temp, i, perm=1;
	while (perm==1){
		perm=0;
		for (i = 0; i < 6; i++) {
			if (tab[i]>tab[i+1]) {
			perm=0;
			temp=tab[i];

			}
		}
	}
}
int main()
{
int *adr_deb,max, i;
adr_deb = malloc(6*sizeof(int));
for (i = 0; i < 6; i++) {
	printf("Entrez la valeur %d :\n",i+1);
	scanf("%d",&adr_deb[i]);
}


getch();
return 0;
}

