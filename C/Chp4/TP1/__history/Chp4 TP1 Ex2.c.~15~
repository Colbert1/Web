#include <stdio.h>
#include <tchar.h>
#include <stdlib.h>
void allocTable (int**tab)
{
	*tab = malloc(6*sizeof(int));
}
void freeTable (int*tab)
{
	free(tab);
}
int main()
{
int *adr_deb,max, i;
adr_deb = malloc(6*sizeof(int));
for (i = 0; i < 6; i++) {
	printf("Entrez la valeur %d :\n",i+1);
	scanf("%d",&adr_deb[i]);
}


getch();
return 0;
}

