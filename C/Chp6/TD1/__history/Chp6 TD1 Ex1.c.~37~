#include <stdio.h>
#include <tchar.h>

typedef struct
{
	char nom[5];
	char prenom[5];
	int age;
	float note;
}
fiche;

int main(int argc, char* argv[])
{
	fiche f1;
	f1.note=12.5;
	strcpy(f1.nom,"Colbert");
	strcpy(f1.prenom,"Gregoire");
	f1.age=18;
	printf("%s"f1.nom);
	printf("%s"f1.prenom);
	printf("%d"f1.age);
	printf("%f"f1.note);
	getch();
	return 0;
}
